@page
@model SweetAlertExamples.Pages.ConfirmationPageModel
@{
}

<script language="javascript" src="js/confirmationPage.js"></script>

<div class="container">

    <form method="post" id="form1">
        <div class="d-grid gap-2 col-6 mx-auto">
            <input type="hidden" asp-for="Confirmation" id="_confirmation" />
         
            <ul class="list-group ms-1">
                <li class="list-group-item list-group-item-primary">Ask question</li>
                <li class="list-group-item list-group-item-info">Set property</li>
                <li class="list-group-item list-group-item-primary">Post</li>
                <li class="list-group-item list-group-item-info">Back to index page</li>
            </ul>
    
            <input type="button" class="btn btn-primary mt-lg-4" value="Question" id="confirmLink" onclick="GetConfirmation()" />
        </div>
    </form>

</div>
