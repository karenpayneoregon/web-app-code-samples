@page
@model PrivacyModel
@{
    ViewData["Title"] = "Privacy Policy";
}
<h1>@ViewData["Title"]</h1>

<p>Use this page to detail your site's privacy policy.</p>


@section Scripts
{
    <script>
        $(document).ready(function(e) {
            $Application.setKeys();
        });
        window.addEventListener('beforeunload', (event) => {
            $Application.removeKeys();
        });
    </script>
}
