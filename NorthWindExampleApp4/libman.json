{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-icons@1.11.3",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "bootstrap-icons.svg",
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.css",
        "font/bootstrap-icons.json",
        "font/bootstrap-icons.min.css",
        "icons/arrow-down-circle.svg",
        "icons/arrow-up-circle.svg"
      ]
    },
    {
      "library": "bootstrap-table@1.22.1",
      "destination": "wwwroot/lib/bootstrap-table/",
      "files": [
        "bootstrap-table.min.js",
        "extensions/sticky-header/bootstrap-table-sticky-header.css",
        "extensions/sticky-header/bootstrap-table-sticky-header.js",
        "extensions/sticky-header/bootstrap-table-sticky-header.min.css",
        "extensions/sticky-header/bootstrap-table-sticky-header.min.js"
      ]
    }
  ]
}