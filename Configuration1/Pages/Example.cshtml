@page
@model Configuration1.Pages.ExampleModel
@{
}

<div class="container">

    <div class="row d-flex justify-content-center align-items-center m-0" style="height: 75vh;">
        <div class="card" style="width: 18rem;">

            <div class="card-body">
                <h5 class="card-title">From appsettings.json</h5>
                <form class="bg-info border p-3 shadow-sm">
                    <div class="col-12">
                        <label asp-for="Contact.FirstName"></label>
                    </div>
                    <div class="col-12">
                        <input asp-for="Contact.FirstName" class="form-control" />
                    </div>

                    <div class="col-12">
                        <label asp-for="Contact.LastName"></label>
                    </div>
                    <div class="col-12">
                        <input asp-for="Contact.LastName" class="form-control" />
                    </div>

                    <div class="col-12">
                        <label asp-for="Contact.EmailAddress"></label>
                    </div>
                    <div class="col-12">
                        <input asp-for="Contact.EmailAddress" class="form-control" />
                    </div>

                    <div class="col-12">
                        <label asp-for="Contact.Region"></label>
                    </div>
                    <div class="col-12">
                        <input asp-for="Contact.Region" class="form-control" />
                    </div>

                </form>
            </div>
        </div>

    </div>
</div>


